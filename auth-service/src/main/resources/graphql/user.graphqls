type User {
	id: ID!
	email: String!
	password: String!
}

type Query {
	allUsers: [User]
}

type Mutation {
	saveUser(addUserInput: AddUserInput): User
	updateUser(updateUserInput: UpdateUserInput): User
}

input AddUserInput {
	email: String!
	password: String!
}

input UpdateUserInput {
	id: ID!
	email: String!
	password: String!
}